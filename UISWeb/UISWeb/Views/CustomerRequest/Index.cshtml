@model IEnumerable<UISWeb.Models.CustomerRequest>

@{
    ViewData["Title"] = "Request";
}

<h2>Request</h2>
<!--
<p>
    <a asp-action="Create">Create New</a>
</p>
-->
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.CustomerId)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Coordinator)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Detail)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Received)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Required)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Completed)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Invoiced)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CustomerId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Coordinator)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Detail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Received)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Required)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Completed)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Invoiced)
            </td>
            <td>
                <!--<a asp-action="Edit" asp-route-id="@item.LabNo">Edit</a> | -->
                <a asp-action="Details" asp-route-id="@item.LabNo">Download</a> 
                <!--<a asp-action="Delete" asp-route-id="@item.LabNo">Delete</a> -->
            </td>
        </tr>
}
    </tbody>
</table>
